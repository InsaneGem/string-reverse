public class LowerUpper 
{
	public static void main(String[] args) 
	{
		String s = "sAnDy";
		char[] ch = s.toCharArray();
		for(int i = 0;i<ch.length;i++)
		{
			int x = ch[i];
			if(x>=97 && x<122)
			{
				x = x-32;
			}
			else if(x>=65 && x<=90)
			{
				x = x+32;
			}	
			char c = (char) x;
			System.out.print(c);				
		}
	}
}
